		{
			"name": "GetBookById",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"const dataJSON = pm.response.json();\r",
							"console.log(\"ID : \" + dataJSON['id']);\r",
							"console.log(\"Name : \" + dataJSON[\"name\"]);\r",
							"console.log(\"Author : \" + dataJSON[\"author\"]);\r",
							"console.log(\"Type : \" + dataJSON[\"type\"]);\r",
							"console.log(\"Price : \" + dataJSON[\"price\"]);\r",
							"console.log(\"Current Stock : \" + dataJSON[\"current-stock\"]);\r",
							"console.log(\"Available : \" + dataJSON[\"available\"]);\r",
							"\r",
							"pm.test('Check Availability', function(){\r",
							"    pm.expect(dataJSON['available']).to.be.equal(true)\r",
							"})\r",
							"\r",
							"pm.test('Check Stock', function(){\r",
							"    pm.expect(dataJSON['current-stock']).to.be.above(0)\r",
							"})"
						],
						"type": "text/javascript"
					}
				}
